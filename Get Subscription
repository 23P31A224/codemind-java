import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	
        int t;
        Scanner read=new Scanner(System.in);
        t=read.nextInt();
        for(int i=0;i<t;i++){
            int a,b,c,d;
            a=read.nextInt();
            if(a>30){
                System.out.println("YES");
            }
            else{
                System.out.println("NO");
            }
        }
	}
}
