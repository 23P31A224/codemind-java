import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
        int t;
        Scanner read=new Scanner(System.in);
        t=read.nextInt();
        while(t-->0){
            int a,b;
            a=read.nextInt();
            b=read.nextInt();
            int c=a/10;
            int res=c*b;
            System.out.printf("%d\n",res);
        }
	}
}
